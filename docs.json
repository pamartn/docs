          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Core concepts",
            "pages": [
              "concepts/quotas",
              "concepts/rate-limits",
              "concepts/error-handling"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/get-posts",
              "api-reference/endpoint/get-profile"
