{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Fetchin API",
  "openapi": "/api-reference/openapi.json",
  "api": {
    "baseUrl": "https://api.fetchin.io",
    "auth": {
      "method": "key",
      "name": "X-API-Key",
      "inputPrefix": ""
    },
    "playground": {
      "mode": "simple"
    }
  },
  "colors": {
    "primary": "#0866C2",
    "light": "#2B8DD9",
    "dark": "#0652A0"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "groups": [
      {
        "group": "Getting Started",
        "pages": [
          "index",
          "quickstart"
        ]
      },
      {
        "group": "Core Concepts",
        "pages": [
          "concepts/quotas",
          "concepts/rate-limits",
          "concepts/error-handling"
        ]
      },
      {
        "group": "Endpoints",
        "pages": [
          "api-reference/endpoint/get-posts",
          "api-reference/endpoint/get-posts-updates",
          "api-reference/endpoint/get-post-comments",
          "api-reference/endpoint/get-profile"
        ]
      }
    ],
    "anchors": [
      {
        "name": "Dashboard",
        "url": "https://fetchin.io/dashboard",
        "icon": "chart-line"
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@fetchin.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://fetchin.io/dashboard"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "cursor"
   ]
  }
}
